<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane fx:id="mainPane" hgap="5.0" vgap="5.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="com.hydro.casting.gui.prod.control.CasterPositionControlController">
    <columnConstraints>
        <ColumnConstraints halignment="LEFT" prefWidth="40"/>
        <ColumnConstraints halignment="CENTER" prefWidth="40"/>
        <ColumnConstraints halignment="CENTER" prefWidth="40"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints vgrow="SOMETIMES"/>
        <RowConstraints vgrow="SOMETIMES"/>
        <RowConstraints vgrow="SOMETIMES"/>
        <RowConstraints vgrow="SOMETIMES"/>
        <RowConstraints vgrow="SOMETIMES"/>
        <RowConstraints vgrow="SOMETIMES"/>
        <RowConstraints vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Label fx:id="position" text="Pos.:??" GridPane.columnSpan="3" GridPane.halignment="CENTER"/>
        <Label text="i.O." GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <Label text="N.i.O." GridPane.columnIndex="2" GridPane.rowIndex="1"/>
        <Label text="Kok.:" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
        <CheckBox fx:id="kokIO" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <CheckBox fx:id="kokNIO" GridPane.columnIndex="2" GridPane.rowIndex="2"/>
        <Label text="AGS.:" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
        <CheckBox fx:id="agsIO" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
        <CheckBox fx:id="agsNIO" GridPane.columnIndex="2" GridPane.rowIndex="3"/>
        <Label text="HKL.:" GridPane.columnIndex="0" GridPane.rowIndex="4"/>
        <CheckBox fx:id="hklIO" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
        <CheckBox fx:id="hklNIO" GridPane.columnIndex="2" GridPane.rowIndex="4"/>
        <Label text="Umgebaut" GridPane.columnIndex="0" GridPane.rowIndex="5" GridPane.columnSpan="2" GridPane.halignment="LEFT"/>
        <CheckBox fx:id="rebuild" GridPane.columnIndex="2" GridPane.rowIndex="5"/>
        <Label fx:id="width" text="1600" GridPane.columnIndex="0" GridPane.rowIndex="6" GridPane.columnSpan="3" GridPane.halignment="CENTER">
            <font>
                <Font size="24"/>
            </font>
        </Label>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</GridPane>
