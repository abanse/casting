<?xml version="1.0" encoding="UTF-8"?>

<?import com.hydro.core.gui.comp.SearchBox?>
<?import com.hydro.core.gui.view.comp.ViewParent?>

<?import com.hydro.core.gui.comp.CacheTreeTableView?>

<?import javafx.scene.control.cell.*?>
<?import javafx.scene.control.*?>

<ViewParent title="Etiketten" prefHeight="500.0"
            prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.111"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.hydro.casting.gui.label.view.LabelViewController">
    <top>
        <ToolBar prefHeight="40.0">
            <SearchBox fx:id="searchBox" prefWidth="200"/>
        </ToolBar>
    </top>
    <left>
        <CacheTreeTableView prefHeight="140.0">
            <columns>
                <TreeTableColumn prefWidth="90.0" text="Name" id="name">
                    <cellValueFactory>
                        <TreeItemPropertyValueFactory property="name"/>
                    </cellValueFactory>
                </TreeTableColumn>
                <TreeTableColumn prefWidth="90.0" text="Probe" id="sampleNumber">
                    <cellValueFactory>
                        <TreeItemPropertyValueFactory property="sampleNumber"/>
                    </cellValueFactory>
                </TreeTableColumn>


                <TreeTableColumn prefWidth="90.0" text="Analyse-Nummer" id="analysisNo">
                    <cellValueFactory>
                        <TreeItemPropertyValueFactory property="analysisNo"/>
                    </cellValueFactory>
                </TreeTableColumn>
                <TreeTableColumn prefWidth="90.0" text="Charge" id="charge">
                    <cellValueFactory>
                        <TreeItemPropertyValueFactory property="charge"/>
                    </cellValueFactory>
                </TreeTableColumn>
                <TreeTableColumn prefWidth="90.0" text="Ofen" id="melter">
                    <cellValueFactory>
                        <TreeItemPropertyValueFactory property="melter"/>
                    </cellValueFactory>
                </TreeTableColumn>
                <TreeTableColumn prefWidth="90.0" text="Legierung" id="alloyName">
                    <cellValueFactory>
                        <TreeItemPropertyValueFactory property="alloyName"/>
                    </cellValueFactory>
                </TreeTableColumn>






            </columns>

        </CacheTreeTableView>
    </left>
</ViewParent>
