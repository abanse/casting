<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.StackPane?>
<StackPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hydro.casting.gui.stock.detail.StockMaterialDetailController">
    <TabPane fx:id="details" styleClass="charge-doc-tab-pane">
        <Tab text="Analyse" closable="false">
            <fx:include fx:id="analysisDetail" source="../control/MaterialAnalysisDetailControl.fxml"/>
        </Tab>
        <Tab text="Merkmale" closable="false">
           <fx:include fx:id="characteristics" source="../control/MaterialCharacteristicsDetailControl.fxml"/>
        </Tab>
    </TabPane>
    <Label fx:id="tabTitle" StackPane.alignment="TOP_RIGHT">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </padding>
    </Label>
</StackPane>
